{
	"pulseaudio#microphone": {
		"format-source-muted": "\udb80\udf6d",
		"format-source": "\udb80\udf6c" ,
		"format": "{format_source}",


		"on-click": "vibranium-core-volume mic mute",
		"on-scroll-up": "vibranium-core-volume mic up",
		"on-scroll-down": "vibranium-core-volume mic down",
		"on-click-right": "vibranium-cmd-launch-terminal 'wiremix --tab input'",

		"scroll-step": 1,
		"tooltip": false
	}
}
